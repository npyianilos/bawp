{
  "name": "@bawp/generators",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@org/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": ["{options.outputPath}"],
        "options": {
          "outputPath": "tools/generators/dist",
          "main": "tools/generators/src/index.ts",
          "tsConfig": "tools/generators/tsconfig.lib.json",
          "rootDir": "tools/generators/src",
          "generatePackageJson": false,
          "assets": [
            {
              "input": "./tools/generators/src",
              "glob": "**/!(*.ts)",
              "output": "."
            },
            {
              "input": "./tools/generators/src",
              "glob": "**/*.d.ts",
              "output": "."
            }
          ]
        }
      }
    }
  },
  "dependencies": {
    "@nx/devkit": "22.5.1",
    "tslib": "^2.3.0"
  },
  "generators": "./generators.json",
  "files": ["dist", "!**/*.tsbuildinfo", "generators.json"]
}
